{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "quotes": "off",
        "semi": "off",
        "no-unused-vars": "off",
        "no-console": "off",
        "no-undef": "off",
        "no-redeclare": "off",
        "no-empty": "off"
    }
}